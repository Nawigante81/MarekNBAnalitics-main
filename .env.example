# ============================================
# NBA Analytics - Environment Example (UJEDNOLICONY)
# Skopiuj do .env i uzupełnij wartości
# ============================================

# --- FRONTEND ---
VITE_API_BASE_URL=http://localhost:8000

# --- SUPABASE (WYMAGANE) ---
SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_key_here
SUPABASE_SERVICE_ROLE_KEY=your_service_role_key_here

# --- APLIKACJA ---
AUTO_SCRAPE_ON_START=false
ENABLE_SCHEDULER=true

# --- API KURSY (OPCJONALNE) ---
ODDS_API_KEY=your_odds_api_key_here

# --- BALDONTLIE (OPCJONALNE) ---
BALLDONTLIE_API_KEY=your_balldontlie_api_key_here

# --- REDIS (OPCJONALNE) ---
REDIS_URL=redis://localhost:6379/0

# --- INNE (opcjonalne) ---
TZ=America/Chicago
VITE_APP_TIMEZONE=America/Chicago
VITE_REFRESH_INTERVAL=30000
VITE_DEV_MODE=true
VITE_DEBUG=false
BASKETBALL_REFERENCE_USER_AGENT=Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36